<?xml version="1.0" encoding="utf-8"?>
<resources>]
    <!-- TEXT -->
    <style name="TXT_CONTENT_WHITE">
        <item name="android:fontFamily">@font/pretendard_r</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:letterSpacing">-0.03</item>
        <item name="android:textSize">14dp</item>
        <item name="android:lineSpacingExtra">4dp</item>
    </style>
    <style name="TXT_CONTENT_WHITE_BOLD">
        <item name="android:fontFamily">@font/pretendard_b</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:letterSpacing">-0.03</item>
        <item name="android:textSize">14dp</item>
        <item name="android:lineSpacingExtra">4dp</item>
    </style>
    <style name="TXT_TITLE">
        <item name="android:fontFamily">@font/pretendard_b</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:letterSpacing">-0.03</item>
        <item name="android:textSize">32dp</item>
        <item name="android:lineSpacingExtra">4dp</item>
    </style>

    <style name="TXT_CONTENT_BLACK">
        <item name="android:fontFamily">@font/pretendard_r</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:letterSpacing">-0.03</item>
        <item name="android:textSize">14dp</item>
        <item name="android:lineSpacingExtra">4dp</item>
    </style>

    <style name="TXT_APP_NAME">
        <item name="android:fontFamily">@font/pretendard_b</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:letterSpacing">-0.03</item>
        <item name="android:textSize">20dp</item>
        <item name="android:lineSpacingExtra">4dp</item>
    </style>

    <style name="TXT_INFO">
        <item name="android:fontFamily">@font/pretendard_r</item>
        <item name="android:textColor">@color/gray9E</item>
        <item name="android:letterSpacing">-0.03</item>
        <item name="android:textSize">14dp</item>
        <item name="android:lineSpacingExtra">4dp</item>
    </style>

    <!-- EditText -->
    <style name="EDTXT_LOGIN" parent="Theme.AppCompat.NoActionBar">
        <item name="colorAccent">@color/blue06</item>
        <item name="android:textColorHint">@color/gray9E</item>
    </style>
</resources>